<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ff9d9e9d" android:endColor="#ff9d9e9d" android:angle="0.0" android:type="linear" android:centerColor="#ff9d9e9d" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ff007dff" android:endColor="#ff007dff" android:angle="0.0" android:type="linear" android:centerColor="#ff007dff" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ff007dff" android:endColor="#ff007dff" android:angle="0.0" android:type="linear" />
            </shape>
        </clip>
    </item>
</layer-list>